# $Id$

prefix=@prefix@
datadir=@datadir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@

INSTALL=@INSTALL@

VPATH=@srcdir@

all:

Ssh.bin:  Ssh.bin.uu
	uudecode Ssh.bin.uu

clean:
	rm -rf Ssh.bin

distprep: Ssh.bin

distclean: clean
	rm -f Makefile *~

install: Ssh.bin
	$(top_srcdir)/mkinstalldirs $(DESTDIR)$(datadir)
	$(INSTALL) -m 0644 -s $(srcdir)/Ssh.bin $(DESTDIR)$(datadir)/Ssh.bin
